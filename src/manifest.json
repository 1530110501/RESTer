{
    "manifest_version": 2,
    "name": "RESTer",
    "version": "2.0.0",
    "description": "A REST client for almost any web service.",
    "icons": {
        "48": "images/icon48.png",
        "96": "images/icon96.png"
    },
    "permissions": [
        "storage",
        "unlimitedStorage",
        "webRequest",
        "webRequestBlocking",
        "<all_urls>"
    ],
    "background": {
        "scripts": [
            "background/lib/base64js.js",
            "background/lib/utils/event-listeners.js",
            "background/lib/utils/fields.js",
            "background/lib/data.js",
            "background/lib/data/utils/data-store.js",
            "background/lib/data/utils/db.js",
            "background/lib/data/utils/migrations.js",
            "background/lib/data/authorization-provider-configurations.js",
            "background/lib/data/authorization-tokens.js",
            "background/lib/data/environments.js",
            "background/lib/data/requests.js",
            "background/lib/data/responses.js",
            "background/lib/data/history.js",
            "background/lib/data/utils/import.js",
            "background/lib/browser-request.js",
            "background/lib/request.js",
            "background/lib/settings.js",
            "background/index.js"
        ]
    },
    "browser_action": {
        "default_icon": {
            "16": "images/icon16.png",
            "24": "images/icon24.png",
            "32": "images/icon32.png"
        }
    }
}
